{
	"name": "voltx",
	"private": true,
	"packageManager": "pnpm@10.7.0",
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"clean": "turbo run clean",
		"format": "prettier --write \"**/*.{ts,tsx,md}\""
	},
	"devDependencies": {
